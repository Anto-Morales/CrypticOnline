{
  "expo": {
    "name": "CRYPTIC-v3.10.5",
    "slug": "CRYPTIC",
    "version": "3.10.5",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "cryptic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n accede a tus fotos para seleccionar im치genes de productos.",
          "cameraPermission": "La aplicaci칩n accede a tu c치mara para tomar fotos de productos."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
